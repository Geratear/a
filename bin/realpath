#!/usr/bin/env python
import os,sys
fp = sys.argv[1]
if not os.path.exists(fp):
    from subprocess import getoutput
    fp = getoutput('which %s' % fp)
    if fp == '':
        quit(fp+'not exists')
for i in range(100):
    print(fp)
    if os.path.islink(fp):
        fp_old = fp
        fp = os.readlink(fp)
        if fp[0] != '/':
            fp = os.path.join(os.path.dirname(fp_old), fp)
        fp = os.path.abspath(fp)
    else:
        break

#print(fp)
#print(os.path.realpath(filepath))
